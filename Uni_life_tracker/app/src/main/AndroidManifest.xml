<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          xmlns:tools="http://schemas.android.com/tools"
          package="com.generic.ult">

    <application
            android:allowBackup="true"
            android:label="@string/app_name"
            android:icon="@drawable/ic_plannr_p_black"
            android:roundIcon="@drawable/ic_plannr_p_black"
            android:supportsRtl="true"
            android:theme="@style/Theme.Plannr">
        <activity android:name=".SettingsActivity"> </activity>
        <activity android:name=".SignUpActivity"> </activity>
        <activity android:name=".AddEventActivity"> </activity>
        <activity android:name=".AddExpensesActivity"> </activity>
        <activity android:name=".ExpensesActivity"> </activity>
        <activity android:name=".SchoolMain"> </activity>
        <activity
                android:name=".MainPageActivity"
                android:exported="true"> </activity>
        <activity
                android:name=".WelcomeActivity"
                android:theme="@style/Theme.Plannr"
                tools:ignore="AllowBackup"
                android:exported="true"
        >
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <meta-data android:name="preloaded_fonts" android:resource="@array/preloaded_fonts"/>
    </application>

</manifest>