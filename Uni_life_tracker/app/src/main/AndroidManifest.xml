<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          xmlns:tools="http://schemas.android.com/tools"
          package="com.generic.ult">

    <application
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/Theme.Uni_life_tracker" tools:ignore="AllowBackup">
        <activity
                android:name=".Add_expenses"
                android:label="@string/title_activity_add_expenses"
                android:theme="@style/Theme.Uni_life_tracker.NoActionBar">
        </activity>
        <activity
                android:name=".expenses_landing"
                android:label="@string/title_activity_expenses_landing"
                android:theme="@style/Theme.Uni_life_tracker.NoActionBar">
        </activity>
        <activity android:name=".Signup">
        </activity>
        <activity android:name=".MainActivity"
						android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <meta-data
                android:name="preloaded_fonts"
                android:resource="@array/preloaded_fonts"/>
    </application>

</manifest>